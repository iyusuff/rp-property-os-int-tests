{
  "name": "iyusuff/property-os-int-tests",
  "description": "Integration test framework for Realpage property os enterprise services",
  "keywords": [
    "integration",
    "test",
    "framework",
    "laravel",
    "lumen"
  ],
  "license": "MIT",
  "type": "project",
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/neomerx/json-api"
    },
    {
      "type": "vcs",
      "url": "https://github.com/iyusuff/rp-property-os-int-tests"
    }
  ],
  "require": {
    "php": "~7.0",
    "laravel/lumen-framework": "5.2.*",
    "vlucas/phpdotenv": "~2.2",
    "neomerx/json-api": "dev-master",
    "realpage/json-api-for-lumen": "^1.0",
    "realpage/enterprise-services-base": "1.2.1",
    "guzzlehttp/guzzle": "~6.0"
  },
  "require-dev": {
    "fzaninotto/faker": "~1.4",
    "phpunit/phpunit": "~5.3",
    "vluzrmos/tinker": "^1.1",
    "squizlabs/php_codesniffer": "^2.6",
    "ddelnano/dredd-hooks-php": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "App\\": "app/"
    }
  },
  "autoload-dev": {
    "classmap": [
      "tests/"
    ]
  },
  "config": {
    "github-oauth": {
      "github.com": ""
    }
  }
}
